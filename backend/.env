# Server Configuration
PORT=3001
NODE_ENV=development

# AWS Workshop Studio Credentials
# Get these from: https://catalog.us-east-1.prod.workshops.aws/event/dashboard/en-US
# Click "Open AWS console" to get temporary credentials
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=<get-from-workshop-console>
AWS_SECRET_ACCESS_KEY=<get-from-workshop-console>
AWS_SESSION_TOKEN=<get-from-workshop-console>

# AWS RDS PostgreSQL Configuration (to be created)
DB_HOST=<your-rds-endpoint>
DB_PORT=5432
DB_NAME=medical_responder
DB_USER=postgres
DB_PASSWORD=<choose-secure-password>
DB_SSL=true

# AWS Cognito Configuration (to be created)
COGNITO_USER_POOL_ID=<to-be-created>
COGNITO_CLIENT_ID=<to-be-created>
COGNITO_ISSUER=https://cognito-idp.us-east-1.amazonaws.com/<user-pool-id>

# AWS IoT Core (to be created)
AWS_IOT_ENDPOINT=<to-be-created>

# AWS S3 (to be created)
S3_BUCKET_NAME=medical-responder-reports-cuhackit2026

# OpenAI Configuration (for Phase 5)
OPENAI_API_KEY=<your-openai-key>
